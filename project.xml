<?xml version="1.0" encoding="utf-8"?>
<project>
	<app title="whathaveidone" file="whathaveidone" main="Main" version="0.1.0" company="MouseMob"/>

	<set name="SWF_VERSION" value="11.8"/>

	<window width="800" height="480" fps="60" background="#000000" hardware="true" vsync="true" resizable="false" antialiasing="4"/>
	<window orientation="portrait"/>
	<window resizable="true" unless="final"/>
	<window allow-shaders="true" if="desktop"/>

	<source path="src"/>

	<assets path="assets/graphics" include="*.png" type="image" embed="true"/>
	<assets path="assets/graphics" include="*.json|*.atlas" type="text" embed="true"/>
	<!--assets path="assets/skins" include="*.png" type="image" embed="true"/-->
	<assets path="assets/fonts" include="*.png" type="image" embed="true"/>
	<assets path="assets/fonts" include="*.fnt" type="text" embed="true"/>
	<assets path="assets/sounds" include="*.wav" type="sound" embed="true"/>
	<assets path="assets/music" include="*.wav|*.ogg" type="music" embed="true"/>

	<haxedef name="source-header" value="haxe"/>

	<haxelib name="spinehaxe"/>
	<haxelib name="SpinePunk"/>
	<haxelib name="HaxePunk"/>
	<haxelib name="HaxePunk-UI"/>
</project>
